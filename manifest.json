{
    "name": "QRL",
    "version": "1.1",
    "description": "Convert your URL or any other text into a QR Code to open in your Mobile Device.",
    "manifest_version": 2,
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "author": "Abhijith Nandagopal",
    "icons":{
        "16":"icon16.png",
        "48":"icon48.png",
        "128":"icon128.png"

    },
    "permissions":[
        "tabs"
    ]
  }